<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="114.0" prefWidth="400.0" stylesheets="@../../../../resources/javaUiStyle.css" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.indisystems.qhmiviewer.model.OpenConnectionController">
   <children>
      <HBox alignment="CENTER_RIGHT" layoutX="10.0" layoutY="10.0" nodeOrientation="LEFT_TO_RIGHT" styleClass="bottomButtonLine" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <Button fx:id="bttOk" mnemonicParsing="false" onMouseClicked="#onBttOkClick" text="%ok">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin></Button>
            <Button fx:id="bttCancel" mnemonicParsing="false" onMouseClicked="#onBttCancelClick" text="%cancel">
               <HBox.margin>
                  <Insets bottom="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <ComboBox fx:id="cbConnections" layoutX="15.0" layoutY="63.0" prefWidth="150.0" AnchorPane.bottomAnchor="62.0" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0" />
      <Label layoutX="14.0" layoutY="38.0" text="%select_a_connection" AnchorPane.bottomAnchor="95.0" />
   </children>
</AnchorPane>
